{
  "name": "cloudfoundry-community/cf-helper-php",
  "description": "Cloudfoundry helper in php",
  "minimum-stability": "stable",
  "license": "MIT",
  "keywords": [
    "cloudfoundry",
    "helper",
    "services",
    "vcapservices"
  ],
  "authors": [
    {
      "name": "Arthur Halet",
      "email": "arthur.halet@orange.com"
    }
  ],
  "require-dev": {
    "predis/predis": "~1.0",
    "mongodb/mongodb": "~1.5",
    "phpunit/phpunit": "6.5.*"
  },
  "autoload": {
    "psr-4": {
      "CfCommunity\\CfHelper\\": "src/CfCommunity/CfHelper/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\CfCommunity\\CfHelper\\": "tests/CfCommunity/CfHelper"
    }
  },
  "suggest": {
    "predis/predis": "Let's you have a redis connection for RedisConnector",
    "mongodb/mongodb": "Use mongodb in for auto connection"
  }
}
